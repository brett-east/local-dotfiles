[alias]
	co = checkout
  ci = commit
	st = status
	br = branch
	unstage = reset HEAD --
	last = log -1 HEAD
	pu = push -u origin
	pr = pull --rebase
	up = fetch && git rebase -i origin/master
	cam = commit -am
	cav = commit -av
